class Mug{
	Mug(int marker){
		System.out.println("Mug ("+marker+")");
	}
	void f(int marker){
		System.out.println("f ("+marker+")");
	}
}
public class Mugs {
	Mug c1;
	Mug c2;
	{
		c1 = new Mug(1);
		c1 = new Mug(2);

	}
	public static void main(String[] args) {
		System.out.println("Inside main()");
		Mugs x = new Mugs();
	}

}
