import java.util.ArrayList;
import java.util.Scanner;

public class searchBean {
	static Scanner sc = new Scanner(System.in);
	public static void main(String[] args) {

		// create an empty array list with an initial capacity
		ArrayList<String> arrlist = new ArrayList<String>();

		// use add() method to add elements in the list
		arrlist.add("Milk");
		arrlist.add("Butter");
		arrlist.add("Cheese");

		// let us print all the elements available in list
		for (Integer number : arrlist) {
			System.out.println("Number = " + number);
		}

		// list contains element 10
		boolean retval = arrlist.contains(0); 

		if (retval == true) {
			System.out.println("element 10 is contained in the list");
		}
		else {
			System.out.println("Sorry search criteria is not listed");
		}

		// list does not contain element 30
		boolean retval2 = arrlist.contains(1);

		if (retval2 == true) {
			System.out.println("element 30 is contained in the list");
		}
		else {
			System.out.println("Sorry search criteria is not listed");
		}
	}

}


