package com.example.localbirds;

public class Robin {

	private String name;
	private int age;

	public Robin(){
		name="Robin";
		age=2;
	}
	
	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}


	

	public static void main(String[] args) {
		Eagle eag = new Eagle();
	}

}
