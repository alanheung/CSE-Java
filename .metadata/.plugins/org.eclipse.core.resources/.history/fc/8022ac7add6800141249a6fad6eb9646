public class EmployeeSysTest {
	private ArrayList<EmployeeSys> empSys;
	
	@Test
	public void addEmployee() {
		Employee emp1 = new Employee();
		empSys.addEmployee(emp1);
		assertEquals(1,empSys.addEmployee());
	}

}
