
public class Student {

    private String name, make, age;
    
    public Student(String name, String make, String age){
        this.name = name;
        this.make = make;
        this.age = age;
    }

	@Override
	public String toString() {
		return "Student [name=" + name + ", make=" + make + ", age=" + age
				+ "]";
	}
}
