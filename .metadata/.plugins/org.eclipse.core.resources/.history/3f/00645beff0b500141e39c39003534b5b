		orchestra[i++] = new Instrument();
		//		orchestra[i++] = new Wind();

	}

	public void tune(Instrument i){

	}

	public void tuneAll(Instrument[] e){
		for(Instrument)
			for(int i=0; e<orchestra.length.i++){
				tune(orchestra[i]);
			}

	}











