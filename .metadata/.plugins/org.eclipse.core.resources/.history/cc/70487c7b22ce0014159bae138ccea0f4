package com.mase.oop1.code2;

public enum FarmerType {
	DAIRY,
	BEEF,
	SHEEP;
	
	private int numAcres;
	
	@Override
	public String toString(){
		String s = "";
		if(this.ordinal()==0){
			s = "Dairy Farmer";
		}else if(this.ordinal()==1){
			s = "Beef Farmer";
		}else if(this.ordinal()==2){
			s = "Sheep Farmer";
		}
		return s;
	}
}
