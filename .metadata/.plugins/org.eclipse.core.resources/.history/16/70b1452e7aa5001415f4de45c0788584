package com.mase.inheritance;

public class Art{
	Art(){
		System.out.println("Art constructor");
		public void methodX(){
			@Override
			public String toString(){
				return "Art::methodX";
			}
		}
	}
	public class Drawing extends Art{
		Drawing()
	}

	public class Cartoon extends Drawing extends Art{

	}
	public static void main(String[] args) {

	}

}
