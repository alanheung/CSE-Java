class Volume{
	private int theLength;
	private int theWidth;
	private int theHeight;
	
	public Volume(){
		theLength=0;
		theWidth=0;
		theHeight=0;
	}
	public Volume(int aLength, int aWidth, int aHeight){
		theLength = aLength;
		theWidth = aWidth;
		theHeight = aHeight;
	}
}
public class VolumeTest {

	public static void main(String[] args) {

	}

}
