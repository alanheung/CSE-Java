import static org.junit.Assert.*;
import static org.junit.Before;
import static org.junit.After;
import static org.junit.Test;

public class StudentGradeTest {
	public static void main(String[] args) {
		private StudentGrade studentGrade;
		@Before
		public void setup(){
			StudentGrade tester = new StudentGrade();
			//System.out.println("In Setup");
		}
		@Test
		public void testGradeAGreaterThan85(){
			assertEquals('A', tester.getGrade(90));
		}
		@Test
		public void testGradeBGreaterThan70(){
			assertEquals('B', tester.getGrade(75));
		}
		@Test
		public void testGradeCGreaterThan55(){
			assertEquals('C', tester.getGrade(65));
		}
		@Test
		public void testGradeDGreaterThan40(){
			assertEquals('D', tester.getGrade(45));
		}
		@Test
		public void testGradeEGreaterThan25(){
			assertEquals('E', tester.getGrade(35));
		}
		@Test
		public void testGradeFGreaterThan0(){
			assertEquals('F', tester.getGrade(15));
		}
	}
}
